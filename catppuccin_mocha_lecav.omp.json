{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "os": "#ACB0BE",
    "closer": "p:os",
    "pink": "#F5C2E7",
    "lavender": "#B4BEFE",
    "blue": "#89B4FA"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "p:os",
          "style": "plain",
          "template": "{{.Icon}} ",
          "type": "os"
        },
        {
          "foreground": "p:blue",
          "style": "plain",
          "template": "{{ .UserName }} ",
          "type": "session"
        },
        {
          "foreground": "p:pink",
          "properties": {
            "folder_icon": "..\ue5fe..",
            "home_icon": "~",
            "style": "agnoster_short"
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "p:lavender",
          "properties": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf594 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "template": " {{ .HEAD }} {{ if .Working.Changed }}{{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<#ef5350> \uf046 {{ .Staging.String }}</>{{ end }}",
          "style": "plain",
          "type": "git"
        },
        {
          "type": "node",
          "style": "plain",
          "powerline_symbol": "\uE0B0",
          "foreground": "p:blue",
          "template": " \uE718 {{ .Full }} "
        },
        {
          "type": "lua",
          "style": "plain",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:blue",
          "template": " \ue620 {{ .Full }} "
        },
        {
          "type": "java",
          "style": "plain",
          "powerline_symbol": "\uE0B0",
          "foreground": "p:blue",
          "template": " \uE738 {{ .Full }}"
        },
        {
          "type": "dotnet",
          "style": "plain",
          "powerline_symbol": "\uE0B0",
          "foreground": "p:blue",
          "template": " \uE77F {{ .Full }} "
        },
        {
          "type": "python",
          "style": "plain",
          "powerline_symbol": "\uE0B0",
          "foreground": "p:blue",
          "template": " \uE235 {{ .Full }} "
        },
        {
          "type": "angular",
          "style": "plain",
          "powerline_symbol": "\uE0B0",
          "foreground": "plain",
          "template": " \uE753 {{ .Full }} "
        },
        {
          "type": "spotify",
          "style": "plain",
          "powerline_symbol": "\uE0B0",
          "foreground": "p:lavender",
          "properties": {
            "playing_icon": "\uE602 ",
            "paused_icon": "\uF8E3 ",
            "stopped_icon": "\uF04D "
          }
        }
      ],
      "type": "prompt"
    },
    {
        "alignment": "left",
        "newline": true,
        "segments": [
            {
                "style": "plain",
                "foreground": "p:closer",
                "template": "\uf105",
                "type": "text"
            }
        ],
        "type": "prompt"
    }
  ],
  "final_space": true, 
  "version": 2
}
