{
  /* COMENTARIOS */
  /*
    C/C++:Enabled Error Squiggles
  */
  /* SETTINGS */
  "workbench.settings.editor": "json",
  "editor.insertSpaces": true,
  "editor.fontLigatures": true,
  "editor.tabSize": 4,
  "editor.renderWhitespace": "all",
  "editor.formatOnPaste": true,
  "editor.formatOnType": false,
  "editor.formatOnSave": true,
  "editor.fontFamily": "Dank Mono",
  "editor.fontSize": 19,
  "files.autoSave": "afterDelay",
  "files.insertFinalNewline": false,
  // UbuntuMono Nerd Font Mono
  // MesloLGLDZ Nerd Font Mono
  // CodeNewRoman Nerd Font Mono
  // Hack NF
  // Monoid Nerd Font
  // Lekton Nerd Font Mono
  // MonoLisa
  // Dank Mono
  // Iosevka
  // CodeNewRoman Nerd Font
  // agave Nerd Font
  // FiraCode Nerd Font Mono
  // CaskaydiaCove Nerd Font Mono
  // Mononoki Nerd Font
  // Monofur Nerd Font
  // Victor Mono
  // Cartograph CF Light
  /* FORMATOS */
  "[csv]": {
    "editor.insertSpaces": false
  },
  /* LENGUAJES/FILES */
  // C++
  "C_Cpp.errorSquiggles": "Enabled",
  "C_Cpp.default.intelliSenseMode": "gcc-x64",
  "C_Cpp.default.compilerPath": "C:\\MinGW\\bin\\g++.exe",
  "C_Cpp.clang_format_path": "C:\\Program Files\\LLVM\\bin\\clang-format.exe",
  "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, IndentWidth: 4, IncludeBlocks: Merge, IndentGotoLabels: true, KeepEmptyLinesAtTheStartOfBlocks: true}",
  "[cpp]": {
    "editor.wordBasedSuggestions": false,
    "editor.suggest.insertMode": "replace",
    "editor.semanticHighlighting.enabled": true,
    "editor.defaultFormatter": "ms-vscode.cpptools",
    "files.insertFinalNewline": true
  },
  // html
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  // javascript
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  // css
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.suggest.insertMode": "replace",
    "editor.tabSize": 2
  },
  // json
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  // markdown
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  // typescript
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  // vue
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  // python
  "[python]": {
    // "editor.defaultFormatter": "",
    "editor.formatOnPaste": false,
    "editor.formatOnSaveMode": "file",
    "files.insertFinalNewline": true
  },
  /* CODE RUNNER */
  "code-runner.runInTerminal": true,
  "code-runner.saveAllFilesBeforeRun": true,
  "code-runner.fileDirectoryAsCwd": true,
  /* POLACODE */
  "polacode.backgroundColor": "#ffffff",
  /* LIVESERVER */
  "liveServer.settings.donotShowInfoMsg": true,
  /* LIVESHARE */
  "liveshare.connectionMode": "relay",
  /* COLORIZE */
  "colorize.languages": [
    "javascript",
    "css",
    "json"
  ],
  /* TABNINE */
  "tabnine.experimentalAutoImports": true,
  /* VIM */
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  // "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.normalModeKeyBindings": [
    {
      "before": [
        "leader",
        "w",
        "w"
      ],
      "commands": [
        "workbench.action.files.save"
      ]
    },
    {
      "before": [
        "leader",
        "q",
        "q"
      ],
      "commands": [
        "workbench.action.closeActiveEditor"
      ]
    },
    {
      "before": [
        "leader",
        "w",
        "q"
      ],
      "commands": [
        "workbench.action.files.save",
        "workbench.action.closeActiveEditor"
      ]
    },
    {
      "before": [
        "leader",
        "c",
        "l"
      ],
      "commands": [
        "editor.action.commentLine"
      ],
      "silent": true
    },
    {
      "before": [
        ":"
      ],
      "commands": [
        "workbench.action.showCommands"
      ],
      "silent": true
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": [
        "leader",
        "c",
        "l"
      ],
      "commands": [
        "editor.action.commentLine"
      ],
      "silent": true
    }
  ],
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-b>": false,
    "<C-h>": false,
    "<C-k>": false,
    "<C-f>": false,
    "<C-j>": false
  },
  /* Custom themes */
  "workbench.colorCustomizations": {
    "[Material Theme Darker]": {
      "editorCursor.foreground": "#ffffff"
    }
  },
  "editor.tokenColorCustomizations": {
    "[Dracula]": {
      "comments": {
        "fontStyle": "italic"
      }
    },
    "[Material Theme Darker]": {
      "comments": {
        // victor mono:
        // "fontStyle": "italic bold"
      },
      "textMateRules": [
        {
          "scope": [
            "constant.numeric.decimal.cpp",
            "constant.numeric",
            "meta.block.cpp",
            "meta.parens.cpp",
            "meta.body.function.definition.cpp",
            "meta.function.definition.cpp",
            "source.cpp"
          ],
          "settings": {
            "foreground": "#ffffff"
          }
        }
      ]
    }
  },
  "editor.semanticTokenColorCustomizations": {
    "[Material Theme Darker]": {
      "rules": {
        "parameter": "#A3E4D7",
        "variable": "#A3E4D7"
      }
    }
  },
  /* TERMINAL */
  // Terminal Integrada
  "terminal.explorerKind": "integrated",
  "terminal.integrated.fontFamily": "Mononoki Nerd Font",
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.tabs.enabled": true,
  "terminal.integrated.defaultLocation": "view",
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "terminal.integrated.profiles.windows": {
    // "Pwsh7": {
    //   "source": "PowerShell7",
    //   "path": "C:\\Users\\lecav\\AppData\\Local\\Microsoft\\WindowsApps\\Microsoft.PowerShell_8wekyb3d8bbwe\\pwsh.exe"
    // },
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell",
      "path": "C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe"
    },
    "Command Prompt": {
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ],
      "args": [],
      "icon": "terminal-cmd"
    },
    "Git Bash": {
      "source": "Git Bash",
      "path": "C:\\Program Files\\Git\\bin\\bash.exe"
    }
  },
  // Terminal Externa
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "workbench.iconTheme": "eq-material-theme-icons-light",
  "window.titleSeparator": " | ",
  "editor.cursorStyle": "line",
  "security.workspace.trust.untrustedFiles": "open",
  // "editor.rulers": [90],
  "editor.lineNumbers": "relative",
  "workbench.colorTheme": "Dracula",
  "workbench.startupEditor": "none",
  "editor.minimap.enabled": false,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "editor.guides.highlightActiveBracketPair": true,
  "window.menuBarVisibility": "compact",
}